<openerp>
    <data>
        <!-- Asset Retirement By Missing -->
        <record
            id="base_sequence_account_asset_retirement_missing"
            model="base.sequence_configurator"
        >
            <field
                name="model_id"
                ref="fixed_asset_retirement_missing.model_account_asset_retirement_missing"
            />
            <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.asset_retirement_missing'),('name','=','name')]"
            />
            <field
                name="fallback_sequence_id"
                ref="fixed_asset_retirement_missing.sequence_asset_retirement_missing"
            />
        </record>

        <record
            id="base_sequence_account_asset_retirement_missing_line"
            model="base.sequence_configurator_line"
        >
            <field
                name="generator_id"
                ref="fixed_asset_retirement_missing.base_sequence_account_asset_retirement_missing"
            />
            <field name="sequence" eval="1" />
            <field
                name="sequence_computation_code"
            >result = document.type_id.sequence_id</field>
        </record>
    </data>
</openerp>

<openerp>
<data>

<record id="account_asset_view_form" model="ir.ui.view">
    <field name="name">Add QR Code</field>
    <field name="model">account.asset.asset</field>
    <field name="inherit_id" ref="account_asset_management.view_account_asset_asset_form"/>
    <field name="arch" type="xml">
        <xpath expr="//notebook" position="inside">
            <page name="qrcode" string="QR Code">
                <field name="qr_image" widget="image"/>
            </page>
        </xpath>
    </field>
</record>

</data>
</openerp>

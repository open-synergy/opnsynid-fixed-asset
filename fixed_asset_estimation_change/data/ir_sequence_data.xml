<openerp>
<data>

<record id="seq_type_fixed_asset_change_estimation_salvage" model="ir.sequence.type">
    <field name="name">Fixed Asset Salvage Value Estimation Change</field>
    <field name="code">account.asset_change_est_sal</field>
</record>

<record
            id="seq_type_fixed_asset_change_estimation_useful_life"
            model="ir.sequence.type"
        >
    <field name="name">Fixed Asset Useful Life Estimation Change</field>
    <field name="code">account.asset_change_est_life</field>
</record>
</data>

<data noupdate="1">
<record id="sequence_fixed_asset_change_estimation_salvage" model="ir.sequence">
    <field name="name">Fixed Asset Salvage Value Estimation Change</field>
    <field name="code">account.asset_change_est_sal</field>
    <field name="padding">5</field>
    <field name="prefix">ESTSAL/%(year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
</record>

<record id="sequence_fixed_asset_change_estimation_useful_life" model="ir.sequence">
    <field name="name">Fixed Asset Useful Life Estimation Change</field>
    <field name="code">account.asset_change_est_life</field>
    <field name="padding">5</field>
    <field name="prefix">ESTLF/%(year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
</record>
</data>

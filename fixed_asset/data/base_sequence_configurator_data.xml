<openerp>
    <data>
        <record
            id="base_sequence_account_asset_asset"
            model="base.sequence_configurator"
        >
            <field name="model_id" ref="fixed_asset.model_account_asset_asset" />
            <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.asset.asset'),('name','=','code')]"
            />
            <field
                name="fallback_sequence_id"
                ref="fixed_asset.sequence_account_asset_asset"
            />
        </record>

        <record
            id="base_sequence_account_asset_asset_line"
            model="base.sequence_configurator_line"
        >
            <field name="generator_id" ref="base_sequence_account_asset_asset" />
            <field name="sequence" eval="1" />
            <field
                name="sequence_computation_code"
            >result = document.category_id.sequence_id</field>
        </record>
    </data>
</openerp>

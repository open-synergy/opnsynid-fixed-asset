<openerp>
<data>

<record id="seq_type_fixed_asset_impairment" model="ir.sequence.type">
    <field name="name">Fixed Asset Impairment</field>
    <field name="code">account.asset.impairment</field>
</record>

<record id="seq_type_fixed_asset_impairment_reversal" model="ir.sequence.type">
    <field name="name">Fixed Asset Impairment Reversal</field>
    <field name="code">account.asset.imp_rev</field>
</record>
</data>

<data noupdate="1">
<record id="sequence_fixed_asset_impairment" model="ir.sequence">
    <field name="name">Fixed Asset Impairment</field>
    <field name="code">account.asset.impairment</field>
    <field name="padding">5</field>
    <field name="prefix">IMP/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>

<record id="sequence_fixed_asset_impairment_reversal" model="ir.sequence">
    <field name="name">Fixed Asset Impairment Reversal</field>
    <field name="code">account.asset.imp_rev</field>
    <field name="padding">5</field>
    <field name="prefix">IREV/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>
</data>
</openerp>

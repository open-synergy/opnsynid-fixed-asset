<openerp>
    <data>
        <!-- Fixed Asset Impairment -->
        <record
            id="base_sequence_fixed_asset_impairment"
            model="base.sequence_configurator"
        >
            <field
                name="model_id"
                ref="fixed_asset_impairment.model_account_asset_impairment"
            />
            <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.asset.impairment'),('name','=','name')]"
            />
            <field
                name="fallback_sequence_id"
                ref="fixed_asset_impairment.sequence_fixed_asset_impairment"
            />
        </record>

        <record
            id="base_sequence_fixed_asset_impairment_line"
            model="base.sequence_configurator_line"
        >
            <field
                name="generator_id"
                ref="fixed_asset_impairment.base_sequence_fixed_asset_impairment"
            />
            <field name="sequence" eval="1" />
            <field
                name="sequence_computation_code"
            >result = document.company_id.fixed_asset_impairment_sequence_id</field>
        </record>

        <!-- Fixed Asset Impairment Reversal-->
        <record
            id="base_sequence_fixed_asset_impairment_reversal"
            model="base.sequence_configurator"
        >
            <field
                name="model_id"
                ref="fixed_asset_impairment.model_account_asset_impairment_reversal"
            />
            <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.asset.impairment_reversal'),('name','=','name')]"
            />
            <field
                name="fallback_sequence_id"
                ref="fixed_asset_impairment.sequence_fixed_asset_impairment_reversal"
            />
        </record>

        <record
            id="base_sequence_fixed_asset_impairment_reversal_line"
            model="base.sequence_configurator_line"
        >
            <field
                name="generator_id"
                ref="fixed_asset_impairment.base_sequence_fixed_asset_impairment_reversal"
            />
            <field name="sequence" eval="1" />
            <field
                name="sequence_computation_code"
            >result = document.company_id.fixed_asset_impairment_reversal_sequence_id</field>
        </record>
    </data>
</openerp>

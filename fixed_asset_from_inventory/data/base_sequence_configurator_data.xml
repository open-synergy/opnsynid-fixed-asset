<openerp>
<data>
<record id="base_sequence_asset_inventory" model="base.sequence_configurator">
    <field
                name="model_id"
                ref="fixed_asset_from_inventory.model_account_asset_from_inventory"
            />
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.asset_from_inventory'),('name','=','name')]"
            />
    <field
                name="fallback_sequence_id"
                ref="fixed_asset_from_inventory.sequence_asset_inventory"
            />
</record>

<record id="base_sequence_asset_inventory_line" model="base.sequence_configurator_line">
    <field name="generator_id" ref="base_sequence_asset_inventory" />
    <field name="sequence" eval="1" />
    <field
                name="sequence_computation_code"
            >result = document.type_id.sequence_id</field>
</record>
</data>
</openerp>

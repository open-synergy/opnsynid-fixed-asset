<odoo>
    <data noupdate="1">
        <!-- INSTALLATION -->
        <record id="complex_fixed_asset_installation_sequence" model="ir.sequence">
            <field name="name">Complex Fixed Asset Installation</field>
            <field name="code">complex_fixed_asset_installation</field>
            <field name="prefix">CFI/%(range_year)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>

        <!-- REMOVAL -->
        <record id="complex_fixed_asset_removal_sequence" model="ir.sequence">
            <field name="name">Complex Fixed Asset Removal</field>
            <field name="code">complex_fixed_asset_removal</field>
            <field name="prefix">CFR/%(range_year)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
    </data>
</odoo>

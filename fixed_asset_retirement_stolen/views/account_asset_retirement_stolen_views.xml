<menuitem
    id="account_asset_retirement_stolen_menu"
    name="Retirement by Stolen"
    parent="account_asset_management_retirement_common.account_asset_retirement_root_menu"
    action="account_asset_retirement_stolen_action"
    sequence="2"/>

